<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
      <t t-inherit="stock_barcode.MainMenu" t-inherit-mode="extension">
          <xpath expr="//div[contains(@class, 'o_main_menu_buttons')]" position="after">
              <div class="row mt-2">
                <div class="col-12">
                    <button
                        class="btn btn-primary w-100 py-3"
                        t-on-click="() => this.actionService.doAction('tasc_stock_barcode.asset_barcode_action')">
                            Audit
                    </button>
                </div>
              </div>
          </xpath>
      </t>
</templates>

