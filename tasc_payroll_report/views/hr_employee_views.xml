<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_employee_form" model="ir.ui.view">
        <field name="name">hr.employee.view.form.inherit.tasc_payroll_report
        </field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form/sheet/group[1]/group[2]/field[@name='coach_id']"
                   position="after">
                <field name="date_of_joining" string="Date of Joining"/>
            </xpath>
        </field>
    </record>
</odoo>