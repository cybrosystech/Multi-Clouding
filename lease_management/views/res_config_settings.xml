<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_res_config_settings_form_inherit" model="ir.ui.view">
            <field name="name">res.config.settings.form.inherit</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="account.res_config_settings_view_form"/>
            <field name="arch" type="xml">

                <xpath expr="//block[@id='analytic']" position="after">
                    <h2>Lease Settings</h2>
                    <div class="row mt16 o_settings_container" id="account_vendor_bills">
                        <div class="col-xs-12 col-md-6 o_setting_box" id="show_purchase_receipts">
                            <div class="o_setting_left_pane">

                            </div>
                            <div class="o_setting_right_pane">
                                <field name="lease_notification_period" />
                                <label for="lease_notification_period"/>
                                <div class="text-muted">
                                    Lease Contract Before Expiration Notifications In Days
                                </div>
                            </div>
                        </div>
                    </div>
                </xpath>

            </field>
        </record>

    </data>
</odoo>