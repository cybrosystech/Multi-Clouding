<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="view_rou_asset_schedule_wizard_form" model="ir.ui.view">
            <field name="name">rou.asset.schedule.wizard.form</field>
            <field name="model">rou.asset.schedule.wizard</field>
            <field name="arch" type="xml">
                <form string="MODEL" create="true" duplicate="true" edit="true" delete="true">
                    <sheet>
                        <group>
                            <field name="date_from"/>
                            <field name="date_to"/>
                            <field name="vendor_ids" widget="many2many_tags"/>
                            <field name="contract_ids" widget="many2many_tags"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="print_report_xlsx" string="Print" type="object" class="oe_highlight"/>
                        <button string="Cancel" special="cancel" class="oe_link"/>
                    </footer>
                </form>
            </field>
        </record>
        
        <record id="view_rou_asset_schedule_wizard_action" model="ir.actions.act_window">
            <field name="name">Rou Asset Schedule</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">rou.asset.schedule.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        
        <menuitem id="menuitem_rou_asset_schedule" name="Rou Asset Schedule" parent="lease_management.menuitem_lease_contract_categ" action="view_rou_asset_schedule_wizard_action" sequence="10"/>
    
    </data>
</odoo>