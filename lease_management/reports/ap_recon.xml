<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_ap_recon_wizard_form" model="ir.ui.view">
            <field name="name">ap.recon.wizard.form</field>
            <field name="model">ap.recon.wizard</field>
            <field name="arch" type="xml">
                <form string="MODEL" create="true" duplicate="true" edit="true" delete="true">
                    <sheet>
                        <group>
                            <field name="date_from"/>
                            <field name="date_to"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="print_report_xlsx" string="Print" type="object" class="oe_highlight"/>
                        <button string="Cancel" special="cancel" class="oe_link"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="view_ap_recon_wizard_action" model="ir.actions.act_window">
            <field name="name">AP Reconcile</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ap.recon.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>


        <menuitem id="menuitem_ap_reconcile" name="AP Reconcile" parent="lease_management.menuitem_lease_contract_categ" action="view_ap_recon_wizard_action" sequence="11"/>

    </data>
</odoo>