<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record model="ir.cron" id="action_profitability_managed_cron">
        <field name="name">Profitability Managed Cron</field>
        <field name="model_id" ref="model_profitability_report_managed"/>
        <field name="state">code</field>
        <field name="code">model.profitability_managed_report(limit="3")</field>
        <field name="active">False</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>

    <record model="ir.cron" id="action_profitability_owned_cron">
        <field name="name">Profitability Owned Cron</field>
        <field name="model_id" ref="model_profitability_report_owned"/>
        <field name="state">code</field>
        <field name="code">model.profitability_owned_report(filter="last_financial_year", limit="3")</field>
        <field name="active">False</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>

    <record model="ir.cron" id="action_profitability_managed_cron_update">
        <field name="name">Profitability Managed Cron Update</field>
        <field name="model_id" ref="model_profitability_report_managed"/>
        <field name="state">code</field>
        <field name="code">model.profitability_managed_cron_update()</field>
        <field name="active">False</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>

    <record model="ir.cron" id="action_profitability_owned_cron_update">
        <field name="name">Profitability Owned Cron Update</field>
        <field name="model_id" ref="model_profitability_report_owned"/>
        <field name="state">code</field>
        <field name="code">model.profitability_owned_cron_update()</field>
        <field name="active">False</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>

</odoo>