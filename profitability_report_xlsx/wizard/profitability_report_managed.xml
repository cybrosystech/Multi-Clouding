<odoo>
    <record id="view_profitability_report_managed_wizard_form" model="ir.ui.view">
        <field name="name">report.profitability.managed.wizard.form</field>
        <field name="model">profitability.report.managed.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="lease_anchor_tenant" widget="many2many_tags"/>
                </group>
                <footer>
                    <button name="generate_xlsx_report" string="Excel"
                            type="object" default_focus="1"
                            class="oe_highlight"/>
<!--                    <button string="Cancel" class="btn btn-secondary"-->
<!--                            special="cancel"/>-->
                </footer>
            </form>
        </field>
    </record>

    <record id="profitable_report_managed_wizard_action" model="ir.actions.act_window">
        <field name="name">Profitability Report</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">profitability.report.managed.wizard</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_profitability_report_managed_wizard_form"/>
        <field name="target">new</field>
    </record>

    <menuitem id="profitable_report_managed_menu_id"
              name="Profitable Report managed"
              parent="account.account_reports_management_menu"
              action="profitable_report_managed_wizard_action"
              sequence="20"
    />

</odoo>