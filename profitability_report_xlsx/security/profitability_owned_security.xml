<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.rule" id="profitability_owned_multi_company_rule">
            <field name="name">Profitability Owned Multi Company</field>
            <field name="model_id" ref="model_profitability_report_owned"/>
            <field name="global" eval="True"/>
            <field name="domain_force">['|', ('company_id', '=', False),
                ('company_id', 'in', company_ids)]
            </field>
        </record>
    </data>
</odoo>