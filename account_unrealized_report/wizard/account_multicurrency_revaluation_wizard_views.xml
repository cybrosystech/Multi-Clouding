<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_account_multicurrency_revaluation_wizard"
            model="ir.ui.view">
        <field name="name">
            account.multicurrency.revaluation.wizard.account_unrealized_report
        </field>
        <field name="model">account.multicurrency.revaluation.wizard</field>
        <field name="inherit_id"
               ref="account_reports.view_account_multicurrency_revaluation_wizard"/>
        <field name="arch" type="xml">
            <xpath expr="//group" position="inside">
                <group>
                    <field name="analytic_account_id"/>
                    <field name="project_site_id"/>
                    <field name="business_unit_id"/>
                    <field name="analytic_distribution"
                           widget="analytic_distribution"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>