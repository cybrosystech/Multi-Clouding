<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="stock_landed_cost_rule_personal" model="ir.rule">
            <field name="name">Personal landed costs</field>
            <field ref="stock_landed_costs.model_stock_landed_cost" name="model_id"/>
            <field name="domain_force">[('create_uid','=',user.id)]</field>
            <field name="groups" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>

        <record id="stock_landed_cost_rule_all" model="ir.rule">
            <field name="name">All Landed Costs</field>
            <field ref="stock_landed_costs.model_stock_landed_cost" name="model_id"/>
            <field name="domain_force">[(1,'=',1)]</field>
            <field name="groups"
                   eval="[(4, ref('stock.group_stock_manager'))]"/>
        </record>
    </data>
</odoo>