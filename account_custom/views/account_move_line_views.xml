<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_account_move_line_filter" model="ir.ui.view">
        <field name="name">account.move.line.filter.account_custom</field>
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_account_move_line_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='unposted']" position="after">
                <filter string="Account between 100000 and 199999"
                        name="account_between_10000_199999"
                        domain="[('account_id.code','&gt;=','100000'),('account_id.code','&lt;=','199999')]"/>
                <filter string="Account between 200000 and 299999"
                        name="account_between_20000_299999"
                        domain="[('account_id.code','&gt;=','200000'),('account_id.code','&lt;=','299999')]"/>
                <filter string="Account between 300000 and 399999"
                        name="account_between_30000_399999"
                        domain="[('account_id.code','&gt;=','300000'),('account_id.code','&lt;=','399999')]"/>
                <filter string="Account between 400000 and 499999"
                        name="account_between_40000_499999"
                        domain="[('account_id.code','&gt;=','400000'),('account_id.code','&lt;=','499999')]"/>
                <filter string="Account between 500000 and 599999"
                        name="account_between_50000_599999"
                        domain="[('account_id.code','&gt;=','500000'),('account_id.code','&lt;=','599999')]"/>
                 <filter string="Account between 600000 and 699999"
                        name="account_between_60000_699999"
                        domain="[('account_id.code','&gt;=','600000'),('account_id.code','&lt;=','699999')]"/>
                <filter string="Account between 700000 and 799999"
                        name="account_between_70000_799999"
                        domain="[('account_id.code','&gt;=','700000'),('account_id.code','&lt;=','799999')]"/>
                <filter string="Account between 800000 and 899999"
                        name="account_between_800000_899999"
                        domain="[('account_id.code','&gt;=','800000'),('account_id.code','&lt;=','899999')]"/>
                <filter string="Account between 900000 and 999999"
                        name="account_between_900000_999999"
                        domain="[('account_id.code','&gt;=','900000'),('account_id.code','&lt;=','999999')]"/>
            </xpath>
        </field>
    </record>

</odoo>