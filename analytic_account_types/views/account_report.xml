<odoo>
    <data>
        <template id="filter_info_template_inherit" inherit_id="account_reports.filter_info_template">
            <xpath expr="//div/div[2]/div[2]" position="after">

                <div class="col-12">
                    <t t-if="options.get('analytic_accounts') != None">
                        Analytic Accounts Project:
                        <t t-set="analytic_account_value">All</t>
                        <t t-foreach="options['selected_analytic_account_project_names']" t-as="analytic_account_project_name">
                            <t t-set="analytic_account_project_value" t-value="''"/>
                            <t t-esc="analytic_account_project_name"/>
                        </t>
                        <t t-esc="analytic_account_project_value"/>
                    </t>
                </div>

            </xpath>
        </template>


    </data>
</odoo>