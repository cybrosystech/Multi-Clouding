<odoo>
    <data>
        <!-- Inherit Form View to Modify it -->
        <record id="view_inherit_view_account_asset_form" model="ir.ui.view">
            <field name="name">view_inherit_view_account_asset_form</field>
            <field name="model">account.asset</field>
            <field name="inherit_id"
                   ref="account_asset.view_account_asset_form"/>
            <field name="arch" type="xml">

                <!-- position: after|before|replace|attributes|inside -->
                <xpath expr="//field[@name='analytic_distribution']"
                       position="before">
                    <field name="analytic_account_id"
                    />
                    <field name="project_site_id"
                    />
                    <!--                    <field name="analytic_distribution"-->
                    <!--                           widget="analytic_distribution"-->
                    <!--                    />-->

                </xpath>

            </field>
        </record>

        <record id="account_assets_view_project_site_search" model="ir.ui.view">
            <field name="name">account.asset.project.site.search</field>
            <field name="model">account.asset</field>
            <field name="inherit_id"
                   ref="account_asset.view_account_asset_search"/>
            <field name="arch" type="xml">
                <field name="model_id" position="after">
                    <field name="project_site_id"/>
                    <field name="site_address"/>
                </field>
            </field>
        </record>
    </data>
</odoo>