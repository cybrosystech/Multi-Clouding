<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record model="ir.cron" id="action_set_remaining_amount">
            <field name="name">Set Remaining Amount</field>
            <field name="model_id" ref="model_account_move_line"/>
            <field name="state">code</field>
            <field name="code">model.action_set_remaining_amount(limit=100)</field>
            <field name="active">True</field>
            <field name="interval_number">1</field>
            <field name="interval_type">months</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

        <record model="ir.cron" id="action_set_remaining_amount_cron_update">
            <field name="name">Set Remaining Amount - Cron Update</field>
            <field name="model_id" ref="model_account_move_line"/>
            <field name="state">code</field>
            <field name="code">model.action_set_remaining_amount_cron_update()</field>
            <field name="active">True</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>