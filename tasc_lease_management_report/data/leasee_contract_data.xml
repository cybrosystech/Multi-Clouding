<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record model="ir.cron" id="action_lease_reports">
            <field name="name">Report - Lease Interest Amortization</field>
            <field name="model_id" ref="model_leasee_management_report"/>
            <field name="state">code</field>
            <field name="code">model.lease_reports_cron(end_limit=50)</field>
            <field name="active">True</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

