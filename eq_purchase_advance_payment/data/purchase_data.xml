<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
         <record model="ir.cron" id="action_set_purchase_id">
        <field name="name">Set Purchase ID on Advance payments </field>
        <field name="model_id" ref="model_purchase_order"/>
        <field name="state">code</field>
        <field name="active">False</field>
        <field name="code">model.action_set_purchase_id(50)</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>

    <record model="ir.cron" id="action_set_purchase_id_cron_update">
        <field name="name">Set Purchase ID on Advance payments - Cron Update</field>
        <field name="model_id" ref="model_purchase_order"/>
        <field name="state">code</field>
        <field name="active">False</field>
        <field name="code">model.action_set_purchase_id_cron_update()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>


    </data>
</odoo>