<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="tasc_bank_reconciliation_report" model="account.report">
        <field name="name">TASC Bank Reconciliation Report</field>
        <field name="filter_show_draft" eval="True"/>
        <field name="filter_date_range" eval="False"/>
        <field name="filter_period_comparison" eval="False"/>
        <field name="filter_hide_0_lines">never</field>
        <field name="search_bar" eval="True"/>
        <field name="default_opening_date_filter">today</field>
        <field name="custom_handler_model_id" ref="model_tasc_account_bank_reconciliation_report_handler"/>
        <field name="column_ids">
            <record id="tasc_bank_reconciliation_report_date" model="account.report.column">
                <field name="name">Date</field>
                <field name="expression_label">date</field>
                <field name="figure_type">date</field>
            </record>
            <record id="tasc_bank_reconciliation_report_label" model="account.report.column">
                <field name="name">Label</field>
                <field name="expression_label">label</field>
                <field name="figure_type">string</field>
            </record>
            <record id="tasc_bank_reconciliation_report_amount_currency" model="account.report.column">
                <field name="name">Amount Currency</field>
                <field name="expression_label">amount_currency</field>
                <field name="figure_type">monetary</field>
            </record>
            <record id="tasc_bank_reconciliation_report_currency" model="account.report.column">
                <field name="name">Currency</field>
                <field name="expression_label">currency</field>
                <field name="figure_type">string</field>
            </record>
            <record id="tasc_bank_reconciliation_report_amount" model="account.report.column">
                <field name="name">Amount</field>
                <field name="expression_label">amount</field>
                <field name="figure_type">monetary</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="tasc_balance_bank" model="account.report.line">
                <field name="name">Bank Balance (TB)</field>
                <field name="code">tasc_balance_bank</field>
                <field name="hierarchy_level">0</field>
                <field name="expression_ids">
                    <record id="tasc_balance_bank_expr" model="account.report.expression">
                        <field name="label">amount</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">tasc_bank_account_balance.amount</field>
                        <field name="auditable" eval="True"/>
                    </record>
                    <record id="tasc_balance_bank_expr_forced_currency_amount" model="account.report.expression">
                        <field name="label">_currency_amount</field>
                        <field name="engine">custom</field>
                        <field name="formula">_report_custom_engine_forced_currency_amount</field>
                        <field name="subformula">amount_currency_id</field>
                    </record>
                </field>
                <field name="children_ids">
                    <record id="tasc_bank_account_balance" model="account.report.line">
                        <field name="name">Bank</field>
                        <field name="code">tasc_bank_account_balance</field>
                        <field name="expression_ids">
                            <record id="tasc_bank_account_balance_amount" model="account.report.expression">
                                <field name="label">amount</field>
                                <field name="engine">custom</field>
                                <field name="formula">_report_custom_engine_bank_account_balance_amount</field>
                                <field name="subformula">amount</field>
                                <field name="auditable" eval="False"/>
                            </record>
                            <record id="tasc_bank_account_balance_forced_currency_amount" model="account.report.expression">
                                <field name="label">_currency_amount</field>
                                <field name="engine">custom</field>
                                <field name="formula">_report_custom_engine_bank_account_balance_amount</field>
                                <field name="subformula">amount_currency_id</field>
                            </record>
                        </field>
                    </record>
                    <record id="tasc_bank_suspense_balance" model="account.report.line">
                        <field name="name">Bank Suspense</field>
                        <field name="code">tasc_bank_suspense_balance</field>
                        <field name="expression_ids">
                            <record id="tasc_bank_suspense_balance_amount" model="account.report.expression">
                                <field name="label">amount</field>
                                <field name="engine">custom</field>
                                <field name="formula">_report_custom_engine_bank_suspense_balance_amount</field>
                                <field name="subformula">amount</field>
                                <field name="auditable" eval="False"/>
                            </record>
                            <record id="tasc_bank_suspense_balance_forced_currency_amount" model="account.report.expression">
                                <field name="label">_currency_amount</field>
                                <field name="engine">custom</field>
                                <field name="formula">_report_custom_engine_bank_suspense_balance_amount</field>
                                <field name="subformula">amount_currency_id</field>
                            </record>
                        </field>
                    </record>

                       <record id="tasc_last_statement_balance" model="account.report.line">
                        <field name="name">Last statement balance</field>
                        <field name="code">tasc_last_statement_balance</field>
                        <field name="hierarchy_level">0</field>
                        <field name="expression_ids">
                            <record id="tasc_last_statement_balance_amount" model="account.report.expression">
                                <field name="label">amount</field>
                                <field name="engine">custom</field>
                                <field name="formula">_report_custom_engine_last_statement_balance_amount</field>
                                <field name="subformula">amount</field>
                                <field name="auditable" eval="False"/>
                            </record>
                            <record id="tasc_last_statement_balance_forced_currency_amount" model="account.report.expression">
                                <field name="label">_currency_amount</field>
                                <field name="engine">custom</field>
                                <field name="formula">_report_custom_engine_last_statement_balance_amount</field>
                                <field name="subformula">amount_currency_id</field>
                            </record>
                        </field>
                    </record>


                    <record id="tasc_difference" model="account.report.line">
                    <field name="name">Difference</field>
                    <field name="code">tasc_differene</field>
                    <field name="hierarchy_level">0</field>
                    <field name="expression_ids">
                        <record id="tasc_difference_bank_expr" model="account.report.expression">
                            <field name="label">amount</field>
                            <field name="engine">aggregation</field>
                            <field name="formula"> tasc_bank_account_balance.amount-tasc_last_statement_balance.amount</field>
                            <field name="auditable" eval="True"/>
                        </record>
                        <record id="tasc_difference_expr_forced_currency_amount" model="account.report.expression">
                            <field name="label">_currency_amount</field>
                            <field name="engine">custom</field>
                            <field name="formula">_report_custom_engine_forced_currency_amount</field>
                            <field name="subformula">amount_currency_id</field>
                            </record>
                        </field>
                    </record>

