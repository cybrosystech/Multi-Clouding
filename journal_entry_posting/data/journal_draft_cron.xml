<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record model="ir.cron" id="account_move_config_cron">
            <field name="name">Account Journal Posting</field>
            <field name="model_id" ref="model_account_move_config"/>
            <field name="state">code</field>
            <field name="active">False</field>
            <field name="code">model.journal_entry_posting()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

        <record model="ir.cron" id="account_move_config_cron_update">
            <field name="name">Account Journal Posting Cron Update</field>
            <field name="model_id" ref="model_account_move_config"/>
            <field name="state">code</field>
            <field name="active">False</field>
            <field name="code">model.journal_entry_posting_cron_update()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

    </data>
</odoo>